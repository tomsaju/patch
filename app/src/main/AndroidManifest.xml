<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.tom.patcher">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".notes.NoteListActivity" />
        <activity android:name=".notes.NoteEditorActivity" />
        <activity android:name=".graph.GraphActivity" />
        <activity android:name=".UnsplashActivity" />
        <activity android:name=".diary.DiaryListActivity" />
        <activity android:name=".diary.DiaryEditorActivity"></activity>
    </application>

</manifest>